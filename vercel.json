{
  "builds": [
    {
      "src": "the-backend/tangoRouletteBackend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "the-backend/tangoRouletteBackend/wsgi.py"
    }
  ]
}
